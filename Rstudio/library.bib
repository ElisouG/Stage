issn = {2167-8359},
journal = {PeerJ},
pages = {e431},
pmid = {24949246},
primaryClass = {10.7287},
title = {{{\textless}i{\textgreater}dDocent{\textless}/i{\textgreater} : a RADseq, variant-calling pipeline designed for population genomics of non-model organisms}},
