file = {:home/elise.gueret/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Oral et al. - 2017 - Gene-centromere mapping in meiotic gynogenetic European seabass.pdf:pdf},
issn = {14712164},
